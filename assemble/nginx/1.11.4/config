sBinFullName='nginx-1.11.4'
sBinDownloadUrl='http://nginx.org/download/'$sBinFullName'.tar.gz'
sBinCodeTar=$sBinFullName'.tar.gz'
sBinSourceCodeSubDir='/'$sBinFullName

sBinCommonLib=''

sBinInstallDir=$sBinInstallRootDir'/'$sBinFullName
sBinLogInstallDir=$sLogInstallRootDir'/nginx'
sBinPidInstallFile=$sBinInstallDir'/nginx.pid'

sBinWorkerProcesses=8
sBinWorkerCpuAffinity='0001 0010 0100 1000 0001 0010 0100 1000'
sBinBaseDomain='weiheinc.com'
sBinBaseDomainReg='weiheinc\.com'
sBinStaticSubDomainReg='(static)|(cms)'

sBinConfigVars='sBinFullName sBinDownloadUrl sBinCodeTar sBinSourceCodeSubDir
sBinCommonLib sBinInstallDir sBinLogInstallDir sBinPidInstallFile
sBinWorkerProcesses sBinWorkerCpuAffinity sBinBaseDomain sBinBaseDomainReg sBinStaticSubDomainReg'